{
  "name": "molder2",
  "description": "Website 2.0",
  "private": true,
  "scripts": {
    "develop": "npm-run-all --parallel develop:**",
    "develop:app": "gatsby develop",
    "develop:functions": "netlify-lambda serve src/lambda",
    "build": "npm-run-all --parallel build:**",
    "build:app": "gatsby build",
    "build:functions": "netlify-lambda build src/lambda",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "gatsby": "next",
    "gatsby-plugin-favicon": "^3.1.4",
    "gatsby-plugin-netlify-cms": "^3.0.0-rc.1",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-typography": "^2.2.0-beta.3",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-source-medium": "^2.0.0",
    "gatsby-transformer-json": "^1.0.20",
    "gatsby-transformer-remark": "^1.7.44",
    "jshint": "^2.9.6",
    "netlify-cms": "^2.1.1",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-moment": "^0.8.1",
    "react-typography": "^0.16.13",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.5",
    "typography": "^0.16.17"
  },
  "proxy": {
    "/.netlify/lambda": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/lambda": ""
      }
    }
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "netlify-lambda": "^0.4.0"
  }
}
