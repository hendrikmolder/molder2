    "build:functions": "netlify-lambda build src/lambda",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.14",
    "@fortawesome/free-solid-svg-icons": "^5.7.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "gatsby": "^2.0.111",
    "gatsby-cli": "^2.4.8",
    "gatsby-plugin-favicon": "^3.1.5",
    "gatsby-plugin-netlify-cms": "^3.0.12",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-sass": "^2.0.0-rc.2",
    "gatsby-plugin-typography": "^2.2.7",
    "gatsby-remark-prismjs": "^3.2.4",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-source-medium": "^2.0.4",
    "gatsby-transformer-json": "^1.0.20",
    "gatsby-transformer-remark": "^1.7.44",
    "jshint": "^2.9.7",
    "netlify-cms": "^2.3.3",
    "netlify-lambda": "^0.4.0",
    "node-sass": "^4.11.0",
    "npm": "^6.6.0",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-moment": "^0.8.4",
    "react-typography": "^0.16.18",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.82.5",
    "typography": "^0.16.18"
  },
  "proxy": {
    "/.netlify/lambda": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/lambda": ""
      }
    }
